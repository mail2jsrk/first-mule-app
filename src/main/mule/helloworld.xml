	<http:listener-config name="HTTP_Listener_config" doc:name="HTTP Listener config" doc:id="233ef7eb-57af-498a-86e1-5a547996d336" >
		<http:listener-connection host="0.0.0.0" port="8081" />
	</http:listener-config>
	<flow name="helloworldFlow" doc:id="5a5c57c6-7e4f-46e7-97f8-56985fb0ba45" >
		<http:listener doc:name="Listener" doc:id="e053c4e2-2474-469a-8f69-ecd3fac4af79" path="/hello" config-ref="HTTP_Listener_config"/>
		<set-payload value="my second commit" doc:name="Set Payload" doc:id="99de8d4b-ded3-479c-9a75-a1e595ec6654" />
		<ee:transform doc:name="Transform Message" doc:id="c4beb526-d79a-453b-9966-d891bb91d628" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/json
---
[2,5,7,5] reduce(item,acc)-> item + acc]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<logger level="INFO" doc:name="Logger" doc:id="7adb4240-8d80-4212-bd34-1b032f5b4888" message="hello mule app"/>
	</flow>
</mule>
